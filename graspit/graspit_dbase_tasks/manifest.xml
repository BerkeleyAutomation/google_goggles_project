<package>
  <description brief="graspit_dbase_tasks">

     Framework for batch processing using GraspIt and tasks stored in a database.

  </description>
  <author>Matei Ciocarlie</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/graspit_dbase_tasks</url>
  <depend package="roscpp"/>
  <depend package="graspit"/>
